path="C:\\F\\"
i=1
j=1
for i in range(1, 100):
    for j in range(1, 100):
        try:
            f=open(path+str(i)+"\\"+str(j)+'.html', 'r')
            g=f.read()
            if len(g)<50:
                print("Folder "+str(i)+" File "+str(j))
            f.close()
        except:
            break
print("Finish")
        
